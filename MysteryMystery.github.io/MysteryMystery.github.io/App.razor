@using MysteryMystery.github.io.Pages

<Router AppAssembly="@typeof(App).Assembly" AdditionalAssemblies="new[]{typeof(MysteryMystery.github.io.Components.Components.CardGroup).Assembly}">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(MainLayout)" />
        <FocusOnNavigate RouteData="@routeData" Selector="h1" />
    </Found>
    <NotFound>
    <LayoutView Layout="@typeof(MainLayout)">
      <_404 />
    </LayoutView>
  </NotFound>

</Router>
